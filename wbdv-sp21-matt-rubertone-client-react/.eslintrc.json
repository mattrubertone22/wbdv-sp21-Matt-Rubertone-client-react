{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "commonjs": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc":"off",
        "no-invalid-this":"off",
        "prefer-const":"off",
        "react/jsx-key":"off",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off"
    },
    "settings": {"react": {
        "version": "detect"
    }}
}
